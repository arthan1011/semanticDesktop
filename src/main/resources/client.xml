<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:arlm="http://artur.lazy-magister.org/properties#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" >

	<!-- File -->
  <owl:Class rdf:about="http://artur.lazy-magister.org/types/file" />

  <!-- Image -->
  <owl:Class rdf:about="http://artur.lazy-magister.org/types/picture">
    <rdfs:subClassOf rdf:resource="http://artur.lazy-magister.org/types/file"/>
    <arlm:fileExtension>png</arlm:fileExtension>
    <arlm:fileExtension>jpg</arlm:fileExtension>
    <arlm:fileExtension>gif</arlm:fileExtension>
  </owl:Class>

  <!-- Document -->
  <owl:Class rdf:about="http://artur.lazy-magister.org/types/document">
    <rdfs:subClassOf rdf:resource="http://artur.lazy-magister.org/types/file"/>
    <arlm:fileExtension>pdf</arlm:fileExtension>
    <arlm:fileExtension>doc</arlm:fileExtension>
    <arlm:fileExtension>docx</arlm:fileExtension>
    <arlm:fileExtension>txt</arlm:fileExtension>
    <arlm:fileExtension>rdf</arlm:fileExtension>
  </owl:Class>

  <!-- Music -->
  <owl:Class rdf:about="http://artur.lazy-magister.org/types/music">
    <rdfs:subClassOf rdf:resource="http://artur.lazy-magister.org/types/file"/>
    <arlm:fileExtension>mp3</arlm:fileExtension>
    <arlm:fileExtension>flac</arlm:fileExtension>
  </owl:Class>

  <!-- Contact -->
  <owl:Class rdf:about="http://artur.lazy-magister.org/types/contact">
    <rdfs:subClassOf rdf:resource="http://artur.lazy-magister.org/types/visible"/>
  </owl:Class>

  <!-- Visible -->
  <owl:Class rdf:about="http://artur.lazy-magister.org/types/visible" />

  <!--Creator prop-->
  <owl:ObjectProperty rdf:about="http://purl.org/dc/elements/1.1/creator">
    <rdfs:label>Кем создан</rdfs:label>
    <rdfs:domain rdf:resource="http://artur.lazy-magister.org/types/document" />
    <rdfs:domain rdf:resource="http://artur.lazy-magister.org/types/picture" />
    <rdfs:range rdf:resource="http://artur.lazy-magister.org/types/contact" />
  </owl:ObjectProperty>

  <!--Like prop-->
  <owl:ObjectProperty rdf:about="http://artur.lazy-magister.org/properties#LIKED_BY">
    <rdfs:label>Кому нравится</rdfs:label>
    <rdfs:domain rdf:resource="http://artur.lazy-magister.org/types/music" />
    <rdfs:range rdf:resource="http://artur.lazy-magister.org/types/contact" />
  </owl:ObjectProperty>

  <!-- Image prop -->
  <owl:ObjectProperty rdf:about="http://artur.lazy-magister.org/properties#IMAGE_OF">
    <rdfs:label>Кто изображен</rdfs:label>
    <rdfs:domain rdf:resource="http://artur.lazy-magister.org/types/picture"/>
    <rdfs:range rdf:resource="http://artur.lazy-magister.org/types/visible"/>
  </owl:ObjectProperty>

  <!-- Email prop -->
  <owl:DatatypeProperty rdf:about="http://artur.lazy-magister.org/properties#EMAIL">
    <rdfs:label>Электронная почта</rdfs:label>
    <rdfs:domain rdf:resource="http://artur.lazy-magister.org/types/contact"/>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#langString"/>
  </owl:DatatypeProperty>
</rdf:RDF>